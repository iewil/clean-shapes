{
  "name": "clean-shapes-2",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:api": "cd .. && node site/node_modules/.bin/wrangler pages dev --port 8788",
    "db:seed": "cd .. && node site/node_modules/.bin/wrangler d1 execute DB --local --file=migrations/0001_initial.sql",
    "db:seed-orders": "cd .. && node site/node_modules/.bin/wrangler d1 execute DB --local --file=migrations/0002_seed_orders.sql",
    "build": "tsc -b && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "lucide-react": "^0.460.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.28.0",
    "zustand": "^5.0.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.15",
    "typescript": "^5.6.3",
    "vite": "^6.0.3",
    "wrangler": "^3.99.0"
  }
}
